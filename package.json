{
  "name": "yyl-sugar-webpack-plugin",
  "version": "0.1.8",
  "engines": {
    "node": ">= 8.0.0"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest ./test/jest/",
    "test:base": "jest ./test/jest/base.test.js",
    "test:hooks": "jest ./test/jest/hooks.test.js",
    "cba": "node ./test/commander.js all --path test/case/base",
    "cbw": "node ./test/commander.js watch --path test/case/base",
    "cha": "node ./test/commander.js all --path test/case/hooks",
    "chw": "node ./test/commander.js watch --path test/case/hooks",
    "cna": "node ./test/commander.js all --path test/case/none",
    "cnw": "node ./test/commander.js watch --path test/case/none"
  },
  "peerDependencies": {
    "webpack": "^4.4.0 || ^5.0.0"
  },
  "devDependencies": {
    "jest": "^25.1.0",
    "webpack": "^4.41.5",
    "yyl-fs": "^0.3.3",
    "yyl-os": "^0.10.2"
  },
  "dependencies": {
    "tapable": "1.1.3",
    "yyl-file-replacer": "0.5.0",
    "yyl-util": "2.3.8"
  },
  "description": "sugar plugin",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/yyl-sugar-webpack-plugin.git"
  },
  "keywords": [
    "sugar",
    "webpack",
    "plugin"
  ],
